<head prefix="og: http://ogp.me/ns#">
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    {% block title %}
        <title>{{ title }}</title>
    {% endblock %}
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {% block stylesheets %}
    {% endblock %}
    {% block nofollow %}
        {% if not production %}
            <meta name="robots" content="noindex, nofollow">
        {% endif %}
    {% endblock %}
</head>
